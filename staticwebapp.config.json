{
    "routes": [
        {
            "route": "/proxy/adt/*",
            "rewrite": "/api/adt"
        },
        {
            "route": "/proxy/blob/*",
            "rewrite": "/api/blob"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/proxy/*",
            "/api/*"
        ]
    }
}