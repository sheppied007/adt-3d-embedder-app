{
    "routes": [
        {
            "route": "/proxy/adt/*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ],
            "rewrite": "/api/proxy/adt"
        },
        {
            "route": "/proxy/blob/*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ],
            "rewrite": "/api/proxy/blob"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*"
        ]
    }
}